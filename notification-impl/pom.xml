<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>eu.scy</groupId>
	<artifactId>notification-impl</artifactId>
	<version>0.1.0-SNAPSHOT</version>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>eu.scy</groupId>
			<artifactId>notification-api</artifactId>
			<version>0.1.0-SNAPSHOT</version>
			<type>jar</type>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>info.collide</groupId>
			<artifactId>sqlspaces-client</artifactId>
			<version>3.0.0</version>
			<type>jar</type>
			<scope>compile</scope>
		</dependency>
	</dependencies>
	<distributionManagement>
		<repository>
			<id>mvn_collide_releases</id>
			<name>Collide Internal Releases</name>
			<url>http://mvn.collide.info/content/repositories/releases</url>
		</repository>
		<snapshotRepository>
			<uniqueVersion>false</uniqueVersion>
			<id>mvn_collide_snapshots</id>
			<name>Collide Internal Snapshots</name>
			<url>http://mvn.collide.info/content/repositories/snapshots</url>
		</snapshotRepository>
	</distributionManagement>
	<repositories>
		<repository>
			<id>snapshots-local</id>
			<url>http://mvn.collide.info/content/repositories/snapshots</url>
		</repository>
		<repository>
			<id>snapshots-tels</id>
			<url>http://repo.telscenter.org/internal_snapshot</url>
		</repository>
		<repository>
			<id>releases-local</id>
			<url>http://mvn.collide.info/content/repositories/releases</url>
		</repository>
	</repositories>
</project>