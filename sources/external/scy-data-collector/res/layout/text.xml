<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/RelativeLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android" android:padding="3dp">
	<EditText android:id="@+id/EditText01" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:hapticFeedbackEnabled="true"
		android:gravity="top|left" android:layout_above="@+id/Button01" android:layout_alignParentTop="true">
	</EditText>
	<Button android:layout_height="wrap_content" android:id="@+id/Button01"
		android:layout_width="fill_parent" android:text="OK"
		android:layout_alignParentBottom="true"></Button>
</RelativeLayout>
