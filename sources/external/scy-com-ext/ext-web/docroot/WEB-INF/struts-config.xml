		<action path="/image_gallery/view_links" forward="portlet.image_gallery.view_links" />
		
		<!-- Wiki -->

		<action path="/wiki/add_tag" type="com.liferay.portlet.wiki.action.AddTagEntryAction">
			<forward name="portlet.wiki.error" path="portlet.wiki.error" />
