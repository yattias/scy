<?xml version="1.0"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>
	<action-mappings>

		<!-- Reports -->

		<action path="/ext/reports/view_reports" type="com.ext.portlet.reports.action.ViewReportsAction">
			<forward name="portlet.ext.reports.view" path="portlet.ext.reports.view" />
			<forward name="portlet.ext.reports.view_reports" path="portlet.ext.reports.view_reports" />
		</action>
		
		
		<!-- Request New -->
		
	
		<action path="/ext/requestsnew/view" type="com.ext.portlet.requestsnew.action.ViewAction">
			<forward name="portlet.ext.requestsnew.view" path="portlet.ext.requestsnew.view" />
		</action>

		<action path="/ext/requestsnew/update_request" type="com.liferay.portlet.requests.action.UpdateRequestAction">
			<forward name="portlet.requests.error" path="portlet.requests.error" />
		</action>
		
		<!-- Asset Publisher -->
		
		<action path="/asset_publisher/add_tag" type="com.ext.portlet.asset_p.action.AddTagEntryAction">
			<forward name="portlet.asset_publisher.view_addTag" path="portlet.asset_publisher.view_addTag" />
		</action>

		<action path="/asset_publisher/add_meta" type="com.ext.portlet.asset_p.action.AddMetadataAction">
			<forward name="portlet.asset_publisher.view_addMetadata" path="portlet.asset_publisher.view_addMetadata" />
		</action>
		
		<action path="/asset_publisher/delete_tag" type="com.ext.portlet.asset_p.action.DeleteTagEntryAction">
		</action>
		
		<action path="/asset_publisher/add_cart" type="com.ext.portlet.asset_p.action.AddCartAction">
		</action>	
		
		<action path="/asset_publisher/link" type="com.ext.portlet.asset_p.action.LinkAction">
			<forward name="portlet.asset_publisher.view_link" path="portlet.asset_publisher.view_link" />
		</action>		
		<action path="/asset_publisher/link_intern" type="com.ext.portlet.asset_p.action.LinkInternAction">
			<forward name="portlet.asset_publisher.view_link" path="portlet.asset_publisher.view_link" />
			<forward name="portlet.asset_publisher.view_linkIntern" path="portlet.asset_publisher.view_linkIntern" />
		</action>			
						
		<action path="/asset_publisher/view_links" forward="portlet.asset_publisher.view_links" />
		
		<!-- Asset by ratio -->
		
	
		<action path="/ext/asset_by_ratio/view" type="com.ext.portlet.assetviewbyratio.action.ViewAction">
			<forward name="portlet.ext.asset_by_ratio.view" path="portlet.ext.asset_by_ratio.view" />
		</action>

		<action path="/ext/asset_by_ratio/view_content" forward="portlet.ext.asset_by_ratio.view_content" />
		
		<action path="/ext/asset_by_ratio/add_tag" type="com.ext.portlet.asset_p.action.AddTagEntryAction">
			<forward name="portlet.asset_publisher.view_addTag" path="portlet.asset_publisher.view_addTag" />
		</action>

		<action path="/ext/asset_by_ratio/add_meta" type="com.ext.portlet.asset_p.action.AddMetadataAction">
			<forward name="portlet.asset_publisher.view_addMetadata" path="portlet.asset_publisher.view_addMetadata" />
		</action>
		
		<action path="/ext/asset_by_ratio/delete_tag" type="com.ext.portlet.asset_p.action.DeleteTagEntryAction">
		</action>
		
		<action path="/ext/asset_by_ratio/add_cart" type="com.ext.portlet.asset_p.action.AddCartAction">
		</action>	
		
		<action path="/ext/asset_by_ratio/link" type="com.ext.portlet.assetviewbyratio.action.LinkAction">
			<forward name="portlet.ext.asset_by_ratio.view_link" path="portlet.ext.asset_by_ratio.view_link" />
		</action>		
		<action path="/ext/asset_by_ratio/link_intern" type="com.ext.portlet.assetviewbyratio.action.LinkInternAction">
			<forward name="portlet.ext.asset_by_ratio.view_link" path="portlet.ext.asset_by_ratio.view_link" />
			<forward name="portlet.ext.asset_by_ratio.view_linkIntern" path="portlet.ext.asset_by_ratio.view_linkIntern" />
		</action>		
		
		<action path="/ext/asset_by_ratio/edit_entry_discussion" type="com.liferay.portlet.messageboards.action.EditDiscussionAction">
			<forward name="portlet.message_boards.error" path="portlet.blogs.error" />
		</action>	
						
		<action path="/ext/asset_by_ratio/view_links" forward="com.ext.portlet.assetviewbyratio.action.ViewAction" />
		
			
		<!-- Book reports -->
		
		<action path="/ext/book_reports/update_request" type="com.liferay.portlet.requests.action.UpdateRequestAction">
			<forward name="portlet.requests.error" path="portlet.requests.error" />
		</action>
		
		<action path="/ext/book_reports/success" 
				forward="portlet.ext.book_reports.success" />
		<action path="/ext/book_reports/error" 
				forward="portlet.ext.book_reports.error" />
		<action path="/ext/book_reports/view" 
				forward="portlet.ext.book_reports.view" />
					     		
		<action path="/ext/book_reports/view_books" type="com.ext.portlet.bookreports.action.AddBookAction">
			<forward name="portlet.ext.book_reports.view" path="portlet.ext.book_reports.view" />
			<forward name="portlet.ext.book_reports.view_books" path="portlet.ext.book_reports.view_books" />
		</action>
		
		<action path="/ext/book_reports/view_reports" type="com.ext.portlet.bookreports.action.ViewBookReportsAction">
			<forward name="portlet.ext.book_reports.view" path="portlet.ext.book_reports.view" />
			<forward name="portlet.ext.book_reports.view_reports" path="portlet.ext.book_reports.view_reports" />
		</action>
		
		<action path="/ext/book_reports/add_book" type="com.ext.portlet.bookreports.action.AddBookAction">
			<forward name="portlet.ext.book_reports.view" path="portlet.ext.book_reports.view" />
			<forward name="portlet.ext.book_reports.error" path="portlet.ext.book_reports.error" />
			<forward name="portlet.ext.book_reports.success" path="portlet.ext.book_reports.success" />			
		</action>
		
		
		<!-- Blog -->
		<action path="/blogs/view" 
				forward="portlet.blogs.view" />
				
		<action path="/blogs/add_tag" type="com.liferay.portlet.blogs.action.AddTagEntryAction">
			<forward name="portlet.blogs.add_tag" path="portlet.blogs.add_tag" />
			<forward name="portlet.blogs.view" path="portlet.blogs.view" />
			<forward name="portlet.blogs.view_entry" path="portlet.blogs.view_entry" />
			<forward name="portlet.blogs.view_addTag" path="portlet.blogs.view_addTag" />
		</action>
		
		<action path="/blogs/delete_tag" type="com.liferay.portlet.blogs.action.DeleteTagEntryAction">
			<forward name="portlet.blogs.delete_tag" path="portlet.blogs.delete_tag" />
			<forward name="portlet.blogs.view" path="portlet.blogs.view" />
			<forward name="portlet.blogs.view_entry" path="portlet.blogs.view_entry" />
		</action>
		
		<action path="/blogs/add_meta" type="com.liferay.portlet.blogs.action.AddMetadataAction">
			<forward name="portlet.blogs.view" path="portlet.blogs.view" />
			<forward name="portlet.blogs.view_entry" path="portlet.blogs.view_entry" />
			<forward name="portlet.blogs.view_addMetadata" path="portlet.blogs.view_addMetadata" />
		</action>		
		<action path="/blogs/link" type="com.liferay.portlet.blogs.action.LinkAction">
			<forward name="portlet.blogs.view" path="portlet.blogs.view" />
			<forward name="portlet.blogs.view_entry" path="portlet.blogs.view_entry" />
			<forward name="portlet.blogs.view_link" path="portlet.blogs.view_link" />
		</action>		
		<action path="/blogs/link_intern" type="com.liferay.portlet.blogs.action.LinkInternAction">
			<forward name="portlet.blogs.view" path="portlet.blogs.view" />
			<forward name="portlet.blogs.view_entry" path="portlet.blogs.view_entry" />
			<forward name="portlet.blogs.view_link" path="portlet.blogs.view_link" />
			<forward name="portlet.blogs.view_linkIntern" path="portlet.blogs.view_linkIntern" />
		</action>			
		<action path="/blogs/add_cart" type="com.liferay.portlet.blogs.action.AddCartAction">
			<forward name="portlet.blogs.view" path="portlet.blogs.view" />
		</action>	
		<action path="/blogs/view_freestylerPreview" type="com.ext.portlet.freestyler.action.ViewFreestylerImageAction">
			<forward name="portlet.ext.freestyler.view_freestylerImage" path="portlet.ext.cart.view_entry" />
		</action>		
				
		<action path="/blogs/view_links" forward="portlet.blogs.view_links" />
		
		<!-- MBMessage -->
				
		<action path="/message_boards/add_tag" type="com.liferay.portlet.messageboards.action.AddTagEntryAction">
			<forward name="portlet.message_boards.view_addTag" path="portlet.message_boards.view_addTag" />
