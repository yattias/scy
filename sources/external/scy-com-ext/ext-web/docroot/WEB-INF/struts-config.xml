		</action>
		<action path="/document_library/add_meta" type="com.liferay.portlet.documentlibrary.action.AddMetadataAction">
			<forward name="portlet.document_library.view_addMetadata" path="portlet.document_library.view_addMetadata" />
			<forward name="portlet.document_library.view" path="portlet.document_library.select_file_entry" />
