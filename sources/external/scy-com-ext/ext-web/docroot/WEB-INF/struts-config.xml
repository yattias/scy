		<action path="/message_boards/view_freestylerPreview" type="com.ext.portlet.freestyler.action.ViewFreestylerImageAction">
			<forward name="portlet.ext.freestyler.view_freestylerImage" path="portlet.ext.cart.view_entry" />
		</action>			
		<action path="/message_boards/view_links" forward="portlet.message_boards.view_links" />		
		
