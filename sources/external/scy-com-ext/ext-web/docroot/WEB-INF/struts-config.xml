		</action>	
		<action path="/message_boards/link" type="com.liferay.portlet.messageboards.action.LinkAction">
			<forward name="portlet.message_boards.view_message" path="portlet.message_boards.view_message" />
			<forward name="portlet.message_boards.view_link" path="portlet.message_boards.view_link" />
