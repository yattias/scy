<!DOCTYPE document SYSTEM "http://google-web-toolkit.googlecode.com/svn/releases/1.5/distro-source/core/src/gwt-module.dtd">
<module>
	<!-- Inherit the GWT stuff. -->
	<inherits name='com.google.gwt.user.User'/>
	<inherits name="com.google.gwt.i18n.I18N"/>

  	<!-- Inherit Edge drawing stuff. -->
  	<inherits name="pl.balon.gwt.diagrams.Diagrams"/>

	<!-- Inherit DnD stuff. -->
  	<inherits name="com.allen_sauer.gwt.dnd.gwt-dnd"/>
  	
  	<inherits name="com.google.gwt.user.theme.standard.Standard"/>
  	<!-- <inherits name="com.google.gwt.user.theme.chrome.Chrome"/> -->
  	<!-- <inherits name="com.google.gwt.user.theme.dark.Dark"/> -->
  	
  	<!-- Specify the app entry point class. -->
	<entry-point class='eu.scy.tools.gstyler.client.GStylerApp'/>
	
	<!-- Specify the available locales -->
	<extend-property name="locale" values="en" />
	<extend-property name="locale" values="de" />
</module>
