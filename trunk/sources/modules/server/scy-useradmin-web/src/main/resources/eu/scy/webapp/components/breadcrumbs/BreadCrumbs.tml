<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">

<div id="breadCrumbsContainer">
    <t:if test="currentProject">
        <t:loop source="objectList" value="currentObject">
            <if test="currentObject">
                <a t:type="links/SelectObjectLink" model="currentObject">${title}</a>
				<img src="${asset:context:graphics/breadcrumb_arrow.png}" class="sep" alt=""/>
            </if>
        </t:loop>
    </t:if>
</div>
</t:container>