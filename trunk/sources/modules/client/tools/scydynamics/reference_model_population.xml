<?xml version="1.0" encoding="UTF-8"?>

<reference_model name="population"
		 notation="system_dynamics"
		 concept_set="population">

  <node id="pop" concept="population"
	        type="stock"
	        low_value="0"
	        high_value="100"
	        default_value="10"
	        />
  <node id="b" concept="births"
	        type="auxiliary"
	        />
  <node id="br" concept="birth_rate"
	        type="constant"
	        low_value="0.01"
	        high_value="0.1"
	        default_value="0.03"
	        />
  <node id="d" concept="deaths"
	        type="auxiliary"
	        />
  <node id="dr" concept="death_rate"
	        type="constant"
	        low_value="0.01"
	        high_value="0.1"
	        default_value="0.02"
	        />

  <node id="pop_in"
  			type="flow"
  			/>
  <node id="pop_out"
  			type="flow"
  			/>

  <edge from="b" to="pop_in"/>
  <edge from="pop_in" to="pop"/>
  <edge from="pop" to="b"/>
  <edge from="br" to="b">
  	<expression type="linear_up" formula="birth_rate"/>
  	<expression type="linear_down" formula="(-birth_rate)"/>
  </edge>
  
  <edge from="d" to="pop_out"/>
  <edge from="pop_out" to="d"/>
  <edge from="pop" to="d"/>
  <edge from="dr" to="d"/>
  
</reference_model>
