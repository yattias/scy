<?xml version="1.0" encoding="UTF-8"?>

<reference_model name="population"
		 notation="system_dynamics"
		 concept_set="population">

  <node id="pop" concept="population"
	        type="stock"
	        low_value="0"
	        high_value="100"
	        default_value="10"
	        />
  <node id="births" concept="births"
	        type="auxiliary"
	        formula="population * birth_rate"
	        />
  <node id="birth_rate" concept="birth_rate"
	        type="constant"
	        low_value="0.01"
	        high_value="0.1"
	        default_value="0.03"
	        />
  <node id="deaths" concept="deaths"
	        type="auxiliary"
	        />
  <node id="death_rate" concept="death_rate"
	        type="constant"
	        low_value="0.01"
	        high_value="0.1"
	        default_value="0.02"
	        />

  <node id="pop_in" type="flow"/>
  <node id="pop_out" type="flow"/>

  <edge from="births" to="pop_in"/>
  <edge from="pop_in" to="population"/>
  <edge from="population" to="births" qualitative="linear"/>
  <edge from="birth_rate" to="births" qualitative="linear"/>
  
  <edge from="deaths" to="pop_out"/>
  <edge from="pop_out" to="deaths"/>
  <edge from="population" to="deaths" qualitative="linear"/>
  <edge from="death_rate" to="deaths" qualitative="linear"/>
  
</reference_model>
