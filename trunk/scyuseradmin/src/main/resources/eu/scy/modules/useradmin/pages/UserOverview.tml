<html t:type="border/layout" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
<head>
    <title>User overdddviddew</title>
</head>
<body>
<h1>User overview</h1>

<table>
    <tr>
        <td valign="top">
            <div t:type="grouplist/GroupList"/>

        </td>
        <td valign="top">

            ${currentlySelectedGroupName}
            <t:grid source="users" row="user">
                <t:parameter name="lastnamecell">
                    <t:pagelink page="start" context="user.username">${user.lastname}</t:pagelink>
                </t:parameter>
            </t:grid>


            <t:PageLink page="Start">Add new user</t:PageLink>
        </td>
    </tr>
</table>
</body>
</html>
