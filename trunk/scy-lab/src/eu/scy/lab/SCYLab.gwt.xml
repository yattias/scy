<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name="com.google.gwt.user.User" />

	<!-- Inherit the GWT Ext stuff.                  -->
	<inherits name="com.gwtext.GwtExt" />

	<!-- Inherit the GWT Ext User Extensions stuff.               -->
	<inherits name="com.gwtextux.GwtExtUx" />

	<!--
		Load the Google Maps GWT bindings from the gwt-google-apis project
	-->
	<inherits name="com.google.gwt.maps.GoogleMaps" />

	<!--
		Load the Google Gears GWT bindings from the gwt-google-apis project
	-->
	<inherits name='com.google.gwt.gears.Gears' />
	<inherits name='com.google.gwt.gears.Offline' />

	<!--  Widget to embed Flash -->
	<inherits name='pl.rmalinowski.gwt2swf.GWT2SWF' />

	<!-- Specify the app entry point class.                   -->
	<entry-point class="eu.scy.lab.client.SCYLab" />

	<!-- <inherits name="com.google.gwt.user.theme.standard.Standard"/>-->
	<!-- <inherits name="com.google.gwt.user.theme.chrome.Chrome"/> -->
	<!-- <inherits name="com.google.gwt.user.theme.dark.Dark"/> -->

	<!-- Specifying the available locales -->
	<extend-property name="locale" values="en" />
	<extend-property name="locale" values="de" />

	<!--
		If you want to deploy this application outside of localhost, you must
		obtain a Google Maps API key at:
		http://www.google.com/apis/maps/signup.html Replace the src attribute
		below with a URL that contains your key.
	-->
	<!--
		script
		src="http://maps.google.com/maps?gwt=1&amp;file=api&amp;v=2.x&amp;key=???"
		/
	-->
	<!-- Google Maps will work for localhost without api key -->
	<script src="http://maps.google.com/maps?gwt=1&amp;file=api&amp;v=2.x" />
	<script
		src="http://maps.google.com/maps?gwt=1&amp;file=api&amp;v=2.x&amp;key=ABQIAAAAZriOtFocn-i7PM7l5RabAhT2yXp_ZAY8_ufC3CFXhHIE1NvwkxR8sdUXeVXDJfsAPFxRl1UeV7Xshw" />

	<servlet class="eu.scy.lab.server.UserManagementImpl" path="/usermanagement" />
	<servlet class="eu.scy.lab.server.tools.map.MapServiceImpl" path="/mapService" />
	<servlet class="eu.scy.lab.server.RepositoryServiceImpl" path="/repositoryService" />
</module>