<!DOCTYPE document SYSTEM "http://google-web-toolkit.googlecode.com/svn/releases/1.5/distro-source/core/src/gwt-module.dtd">
<module>

	<!-- Inherit the GWT Ext stuff. -->
	<inherits name="com.gwtext.GwtExt" />
	
	<!-- Inherit Google Maps API. -->
	<inherits name="com.google.gwt.maps.GoogleMaps" />
	<script src="js/gears_init.js" />
	
 	<!-- Google Maps Key for http://scy.collide.info -->
	<script src="http://maps.google.com/maps?gwt=1&amp;file=api&amp;v=2.x&amp;key=ABQIAAAAsEbwtUFaUXOhRtKP92djAhSqYo9mqsAy07a_h_oDd-3qDOc2kRRo3x8jkJDwFf8ZNvWp83ikhnD0lg" />
	
	<!-- Inherit the Gears API. -->
	<inherits name='com.google.gwt.gears.Gears' />
	<inherits name='com.google.gwt.gears.Offline' />
	
	<!-- Add Stylesheet and Script needed for Ext.  -->  
  	<stylesheet src="js/ext/resources/css/ext-all.css" />
	<script src="js/ext/adapter/ext/ext-base.js" />
	<script src="js/ext/ext-all.js" />
  		
  	<servlet class="eu.scy.tools.map.server.MapServiceImpl" path="/mapService" />
</module>
