<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>
	
	<!-- GWT HTTP support -->
	<inherits name="com.google.gwt.http.HTTP"/>

	<!-- Inherit the GWT Ext stuff. -->
	<inherits name="com.gwtext.GwtExt"/>

	<!-- Inherit the GWT Ext UX stuff. -->
	<inherits name="com.gwtextux.GwtExtUx"/>
	<inherits name="com.gwtextux.ManagedIFrame"/>
	
	<!-- Inherit The Rocket Stuff --> 
	<inherits name="rocket.selection.Selection"/>
	<inherits name="rocket.style.Style"/>
	
	<!-- Add Stylesheet and Script needed for Ext.  -->  
  	<stylesheet src="js/ext/resources/css/ext-all.css"/>
	<script src="js/ext/adapter/ext/ext-base.js"/>
	<script src="js/ext/ext-all.js"/>
  
  	<inherits name="com.google.gwt.user.theme.standard.Standard"/>
  	<!-- <inherits name="com.google.gwt.user.theme.chrome.Chrome"/> -->
  	<!-- <inherits name="com.google.gwt.user.theme.dark.Dark"/> -->
  	
</module>
