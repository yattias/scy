<model xmlns="http://www.co-lab.nl/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.co-lab.nl/xml Model.xsd">
  <descriptor>
    <symbol>model</symbol>
  </descriptor>
  <variables>
    <varspec>
      <variable>
        <descriptor>
          <symbol>startExercise</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>30</expression>
          <unit>?</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>ExerciseLevel</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>0.4</expression>
          <unit>?</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>InsulineConcentration</symbol>
        </descriptor>
        <type>double</type>
        <color>#008000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <stockSpec>
          <initial>0</initial>
          <unit>micmol/l</unit>
          <inflow>PancreasInsulineProduction</inflow>
          <outflow>InsulineDecay</outflow>
        </stockSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>Exercise</symbol>
        </descriptor>
        <type>double</type>
        <color>#ffc800</color>
        <visible>true</visible>
      </variable>
      <specification>
        <auxSpec>
          <expression>ExerciseLevel*(1/(1+exp(-10*(time-startExercise))) - 1/(1+exp(-10*(time-stopExercise))))</expression>
          <unit>?</unit>
          <exprType>0</exprType>
        </auxSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>time</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <timeSpec>
          <unit>?</unit>
          <time start="0.0" stop="90.0" />
          <integration method="euler" step="0.1" />
        </timeSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>InsulineBreakdownRate</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>0.005</expression>
          <unit>?</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>InsulineDecayRate</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>0.1</expression>
          <unit>?</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>MetabolismRate</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>0.3</expression>
          <unit>?</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>InsulineBreakdown</symbol>
        </descriptor>
        <type>double</type>
        <color>#000080</color>
        <visible>true</visible>
      </variable>
      <specification>
        <auxSpec>
          <expression>GlucoseConcentration*InsulineConcentration *InsulineBreakdownRate</expression>
          <unit>?</unit>
          <exprType>0</exprType>
        </auxSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>TargetGlucoseLevel</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>4</expression>
          <unit>?</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>GlucoseInDigestiveSystem</symbol>
        </descriptor>
        <type>double</type>
        <color>#ff0000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <stockSpec>
          <initial>30</initial>
          <unit>?</unit>
          <outflow>GlucoseIntake</outflow>
        </stockSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>InsulineDecay</symbol>
        </descriptor>
        <type>double</type>
        <color>#9a7c32</color>
        <visible>true</visible>
      </variable>
      <specification>
        <auxSpec>
          <expression>InsulineConcentration * InsulineDecayRate</expression>
          <unit>?</unit>
          <exprType>0</exprType>
        </auxSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>GlucoseIntake</symbol>
        </descriptor>
        <type>double</type>
        <color>#7c009f</color>
        <visible>true</visible>
      </variable>
      <specification>
        <auxSpec>
          <expression>GlucoseIntakeRate*GlucoseInDigestiveSystem</expression>
          <unit>?</unit>
          <exprType>0</exprType>
        </auxSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>InsulineProductionCoefficient</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>0.05</expression>
          <unit>?</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>MuscleMetabolism</symbol>
        </descriptor>
        <type>double</type>
        <color>#ef1385</color>
        <visible>true</visible>
      </variable>
      <specification>
        <auxSpec>
          <expression>Exercise * MetabolismRate</expression>
          <unit>?</unit>
          <exprType>0</exprType>
        </auxSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>productionInLiverRate</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>0.1</expression>
          <unit>?</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>GlucoseConcentration</symbol>
        </descriptor>
        <type>double</type>
        <color>#fa0000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <stockSpec>
          <initial>4</initial>
          <unit>mmol/l</unit>
          <inflow>GlucoseIntake</inflow>
          <inflow>ProductionInLiver</inflow>
          <outflow>InsulineBreakdown</outflow>
          <outflow>MuscleMetabolism</outflow>
        </stockSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>stopExercise</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>55</expression>
          <unit>?</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>GlucoseIntakeRate</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>0.0053</expression>
          <unit>?</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>ProductionInLiver</symbol>
        </descriptor>
        <type>double</type>
        <color>#f46510</color>
        <visible>true</visible>
      </variable>
      <specification>
        <auxSpec>
          <expression>max (TargetGlucoseLevel-GlucoseConcentration, 0)*  productionInLiverRate</expression>
          <unit>?</unit>
          <exprType>0</exprType>
        </auxSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>PancreasInsulineProduction</symbol>
        </descriptor>
        <type>double</type>
        <color>#2c92bb</color>
        <visible>true</visible>
      </variable>
      <specification>
        <auxSpec>
          <expression>max(GlucoseConcentration - TargetGlucoseLevel,0)*InsulineProductionCoefficient</expression>
          <unit>?</unit>
          <exprType>0</exprType>
        </auxSpec>
      </specification>
    </varspec>
