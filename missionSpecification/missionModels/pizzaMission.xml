<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

   <import resource="eloToolConfigs.xml"/>

   <bean name="missionConfigInput" class="eu.scy.client.desktop.scydesktop.tools.mission.springimport.BasicMissionConfigInput">

      <property name="eloToolConfigs" ref="eloToolConfigs"/>

      <property name="basicMissionMap">
         <bean class="eu.scy.client.desktop.scydesktop.config.BasicMissionMap">
            <property name="initialLasId" value="methodologySteps"/>
            <property name="loEloUris">
               <list />
            </property>
            <property name="lasses">
               <list>
                  <bean class="eu.scy.client.desktop.scydesktop.config.BasicLas">
                     <property name="id" value="methodologySteps"/>
                     <property name="xPosition" value="0"/>
                     <property name="yPosition" value="0"/>
                     <property name="tooltip" value="Methodology Steps"/>
                     <property name="lasType" value="ORIENTATION"/>
                     <property name="instructionUri" value="http://www.scy-lab.eu/content/en/mission3/Part7_Goals"/>
                     <property name="loEloUris">
                        <list />
                     </property>
                     <property name="nextLasses">
                        <list>
                           <value>discussion</value>
                           <value>criteria</value>
                        </list>
                     </property>
                     <property name="anchorEloId" value="methodologySteps"/>
                     <property name="intermediateEloIds">
                        <list />
                     </property>
                  </bean>
                  
				  <bean class="eu.scy.client.desktop.scydesktop.config.BasicLas">
                     <property name="id" value="discussion"/>
                     <property name="xPosition" value="50"/>
                     <property name="yPosition" value="40"/>
                     <property name="tooltip" value="Discussion Notes"/>
                     <property name="lasType" value="DEBATE"/>
                     <property name="instructionUri" value="http://www.scy-lab.eu/content/en/mission3/Part1_Goals"/>
                     <property name="loEloUris">
                        <list />
                     </property>
                     <property name="nextLasses">
                        <list>
                           <value>methodologySteps</value>
                           <value>criteria</value>
                        </list>
                     </property>
                     <property name="anchorEloId" value="discussionNotes"/>
                     <property name="intermediateEloIds">
                        <list />
                     </property>
                  </bean>
				  
				  <bean class="eu.scy.client.desktop.scydesktop.config.BasicLas">
                     <property name="id" value="criteria"/>
                     <property name="xPosition" value="-50"/>
                     <property name="yPosition" value="40"/>
                     <property name="tooltip" value="Criteria"/>
                     <property name="lasType" value="REFLECTION"/>
                     <property name="instructionUri" value="http://www.scy-lab.eu/content/en/mission3/Part7_Goals"/>
                     <property name="loEloUris">
                        <list />
                     </property>
                     <property name="nextLasses">
                        <list>
                           <value>discussion</value>
                           <value>methodologySteps</value>
                        </list>
                     </property>
                     <property name="anchorEloId" value="criteriumWeightTable"/>
                     <property name="intermediateEloIds">
                        <list />
                     </property>
                  </bean>
               </list>
            </property>
         </bean>
      </property>


      <property name="basicMissionAnchors">
         <list>
            <bean class="eu.scy.client.desktop.scydesktop.config.BasicMissionAnchor">
               <property name="id" value="methodologySteps"/>
               <property name="uri" value="roolo://memory/30003/0/Methodology+Steps.scyxproc"/>
               <property name="targetDescriptionUri" value="http://www.scy-lab.eu/content/en/mission3/Methodology_Steps.txt"/>
               <property name="assignmentUri" value="http://www.scy-lab.eu/content/en/mission3/Part7_ELO7.1.1.html"/>
               <property name="resourcesUri" value="http://www.scy-lab.eu/content/en/mission3/Part7_Optimization7.2.html"/>
               <property name="inputMissionAnchorIds">
                  <list />
               </property>
               <property name="loEloUris">
                  <list />
               </property>
            </bean>

            <bean class="eu.scy.client.desktop.scydesktop.config.BasicMissionAnchor">
               <property name="id" value="discussionNotes"/>
               <property name="uri" value="roolo://memory/30002/0/Discussion+notes.rtf"/>
               <property name="targetDescriptionUri" value="http://www.scy-lab.eu/content/en/mission3/Discussion_Notes.txt"/>
               <property name="assignmentUri" value="http://www.scy-lab.eu/content/en/mission3/Part7_ELO7.2.2.html"/>
               <property name="resourcesUri" value="http://www.scy-lab.eu/content/en/mission3/Part7_Optimization7.2.html"/>
               <property name="inputMissionAnchorIds">
                  <list />
               </property>
               <property name="loEloUris">
                  <list />
               </property>
            </bean>
		
            <bean class="eu.scy.client.desktop.scydesktop.config.BasicMissionAnchor">
               <property name="id" value="criteriumWeightTable"/>
               <property name="uri" value="roolo://memory/30001/0/Criterium+Weight+Table.scypds"/>
               <property name="targetDescriptionUri" value="http://www.scy-lab.eu/content/en/mission3/Criterium_Weight_Table.txt"/>
               <property name="assignmentUri" value="http://www.scy-lab.eu/content/en/mission3/Part7_ELO7.2.3.2.html"/>
               <property name="resourcesUri" value="http://www.scy-lab.eu/content/en/mission3/Part7_Optimization7.2.html"/>
               <property name="inputMissionAnchorIds">
                  <list />
               </property>
               <property name="loEloUris">
                  <list />
               </property>
            </bean>
		</list>
			
      </property>


      <property name="templateEloUris">
         <list>
            <value>roolo://memory/20109/0/Empty_concept_map.scymapping</value>
            <value>roolo://memory/20110/0/Empty_dataset.dataset</value>
            <value>roolo://memory/20111/0/Empty_experimental_procedure.scyxproc</value>
            <value>roolo://memory/20112/0/Empty_plain_text.text</value>
            <value>roolo://memory/20113/0/Empty_rich_text.rtf</value>
         </list>
      </property>

   </bean>

</beans>
