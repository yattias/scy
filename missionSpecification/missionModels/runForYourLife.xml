                     <property name="nextLasses">
                        <list>
                           <value>conceptualization</value>
                        </list>
                     </property>
                     <property name="anchorEloId" value="listOfInformation"/>
					 
                     <property name="intermediateEloIds">
                        <list>
						</list>
                     </property>
                  </bean>

                  <bean class="eu.scy.client.desktop.scydesktop.config.BasicLas">
                     <property name="id" value="conceptualization"/>
                     <property name="xPosition" value="50"/>
                     <property name="yPosition" value="-50"/>
                     <property name="tooltip" value="tip_LAS_B"/>
                     <property name="lasType" value="CONCEPTUALIZATION"/>
                     <property name="instructionUri" value="http://www.scy-lab.eu/content/en/mission5/curtain_CONCEPTUALIZATION.html"/>
                     <property name="loEloUris">
                        <list />
                     </property>
                     <property name="nextLasses">
                        <list>
                           <value>information</value>
                           <value>experiment</value>
                        </list>
                     </property>
                     <property name="anchorEloId" value="hypothesis"/>
                     <property name="intermediateEloIds">
                        <list>
						</list>
                     </property>
                  </bean>

                  <bean class="eu.scy.client.desktop.scydesktop.config.BasicLas">
                     <property name="id" value="experiment"/>
                     <property name="xPosition" value="50"/>
                     <property name="yPosition" value="50"/>
                     <property name="tooltip" value="tip_LAS_C"/>
                     <property name="lasType" value="EXPERIMENT"/>
                     <property name="instructionUri" value="http://www.scy-lab.eu/content/en/mission5/curtain_EXPERIMENT.html"/>
                     <property name="loEloUris">
                        <list />
                     </property>
                     <property name="nextLasses">
                        <list>
                           <value>conceptualization</value>
                           <value>report</value>
                        </list>
                     </property>
                     <property name="anchorEloId" value="conclusionOfExperiment"/>
                     <property name="intermediateEloIds">
                        <list>
								<value>visualRepresentationOfData</value>
							</list>
                     </property>
                  </bean>
                  
				  <bean class="eu.scy.client.desktop.scydesktop.config.BasicLas">
                     <property name="id" value="report"/>
                     <property name="xPosition" value="-50"/>
                     <property name="yPosition" value="50"/>
                     <property name="tooltip" value="tip_LAS_D"/>
                     <property name="lasType" value="REPORT"/>
                     <property name="instructionUri" value="http://www.scy-lab.eu/content/en/mission5/curtain_REPORT.html"/>
                     <property name="loEloUris">
                        <list />
                     </property>
                     <property name="nextLasses">
                        <list>
                           <value>experiment</value>
                        </list>
                     </property>
                     <property name="anchorEloId" value="report"/>
                     <property name="intermediateEloIds">
                        <list>
                        </list>
                     </property>
                  </bean>
				  
               </list>
            </property>
         </bean>
      </property>


