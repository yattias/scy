               <property name="targetDescriptionUri" value="http://www.scy-lab.eu/content/en/mission5/tip_LAS_C.txt"/>
               <property name="assignmentUri" value="http://www.scy-lab.eu/content/en/mission5/instruction_3_visualRepresentationOfData.html"/>
               <property name="resourcesUri" value=""/>
               <property name="inputMissionAnchorIds">
                  <list />
               </property>
               <property name="loEloUris">
                  <list />
               </property>
            </bean>
		
            <bean class="eu.scy.client.desktop.scydesktop.config.BasicMissionAnchor">
               <property name="id" value="conclusionOfExperiment"/>
               <property name="uri" value="roolo://memory/50007/0/Conclusion+of+experiment.rtf"/>
			   <property name="iconType" value="Dataset_processed"/>
               <property name="targetDescriptionUri" value="http://www.scy-lab.eu/content/en/mission5/tip_LAS_C.txt"/>
               <property name="assignmentUri" value="http://www.scy-lab.eu/content/en/mission5/instruction_4_conclusionOfExperiment.html"/>
               <property name="resourcesUri" value=""/>
               <property name="inputMissionAnchorIds">
                  <list />
               </property>
               <property name="loEloUris">
                  <list />
               </property>
            </bean>
		
            <bean class="eu.scy.client.desktop.scydesktop.config.BasicMissionAnchor">
               <property name="id" value="report"/>
