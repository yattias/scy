<!DOCTYPE document SYSTEM "http://google-web-toolkit.googlecode.com/svn/releases/1.5/distro-source/core/src/gwt-module.dtd">
<module>

	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User'/>

	<!--  Inherit the ODCGWT stuff. -->
	<inherits name='com.rednels.ofcgwt.OFCGWT'/>

  	<inherits name="com.google.gwt.user.theme.standard.Standard"/>

	<!-- Specify the app entry point class. -->
	<entry-point class='eu.scy.monitor.client.Statistics'/>
</module>
