                 <table style="border: 1px solid #CCC; width: 100%; padding: 5px;">
                   	  <input name="<portlet:namespace />title" size="35" type="hidden" value="<%= metadataEntry.getDc_title() %>" />
            		  <input name="<portlet:namespace />title" size="35" type="hidden" value="<%= metadataEntry.getDc_description() %>" />
           			  <input name="<portlet:namespace />publisher" size="35" type="hidden" value="<%= metadataEntry.getDc_publisher() %>">
           			  <input name="<portlet:namespace />creator" size="35" type="hidden" value="<%= metadataEntry.getDc_creator() %>">
           			  <input name="<portlet:namespace />contributor" size="35" type="hidden" value="<%= metadataEntry.getDc_contributor() %>">
           			  <input name="<portlet:namespace />coverage" size="35" type="hidden" value="<%= metadataEntry.getDc_coverage() %>">
           			  <input name="<portlet:namespace />format" size="35" type="hidden" value="<%= metadataEntry.getDc_format() %>">
           			  <tr>
                        <td style="color: #12558E; padding: 5px;" title="<%= LanguageUtil.get(themeDisplay.getLocale(), "metadata.language_tooltip") %>"> Language <img src="/html/themes/classic/images/common/help.png" class="icon" /></td>
                        <td style="color: #12558E; padding: 5px;">
					           <input type="radio" name="<portlet:namespace />language" value="en"<%= "en".equals(metadataEntry.getDc_language()) ? " checked" : "" %>> EN
					           <input type="radio" name="<portlet:namespace />language" value="de"<%= "de".equals(metadataEntry.getDc_language()) ? " checked" : "" %>> DE
					           <input type="radio" name="<portlet:namespace />language" value="nl"<%= "nl".equals(metadataEntry.getDc_language()) ? " checked" : "" %>> NL
					           <input type="radio" name="<portlet:namespace />language" value="fr"<%= "fr".equals(metadataEntry.getDc_language()) ? " checked" : "" %>> FR
					           <input type="radio" name="<portlet:namespace />language" value="no"<%= "no".equals(metadataEntry.getDc_language()) ? " checked" : "" %>> NO
					           <input type="radio" name="<portlet:namespace />language" value="cy"<%= "cy".equals(metadataEntry.getDc_language()) ? " checked" : "" %>> CY
      				  </td>     
                      </tr>                      
                      <input name="<portlet:namespace />relation" size="35" type="hidden" value="<%= metadataEntry.getDc_relation() %>">
                      <input name="<portlet:namespace />coverage" size="35" type="hidden" value="<%= metadataEntry.getDc_rights() %>">
                      <input name="<portlet:namespace />identifier" size="35" type="hidden" value="<%= metadataEntry.getDc_identifier() %>">
                      <tr>
                        <td style="color: #12558E; padding: 5px;" title="<%= LanguageUtil.get(themeDisplay.getLocale(), "metadata.subject_tooltip") %>"> Age <img src="/html/themes/classic/images/common/help.png" class="icon" /></td>
                        <td style="color: #12558E; padding: 5px;">                       
					           <input type="checkbox" name="<portlet:namespace />subject" value="1012"<%= metadataEntry.getDc_subject().contains("1012") ? " checked" : "" %>> 10-12
					           <input type="checkbox" name="<portlet:namespace />subject" value="1214"<%= metadataEntry.getDc_subject().contains("1214") ? " checked" : "" %>> 12-14
					           <input type="checkbox" name="<portlet:namespace />subject" value="1416"<%= metadataEntry.getDc_subject().contains("1416") ? " checked" : "" %>> 14-16
					           <input type="checkbox" name="<portlet:namespace />subject" value="1618"<%= metadataEntry.getDc_subject().contains("1618") ? " checked" : "" %>> 16-18					                   
					    </td>
                      </tr> 
                      <tr>
                        <td style="color: #12558E; padding: 5px;" title="<%= LanguageUtil.get(themeDisplay.getLocale(), "metadata.type_tooltip") %>"> Type <img src="/html/themes/classic/images/common/help.png" class="icon" /></td>
                        <td style="color: #12558E; padding: 5px;">                       
					           <input type="checkbox" name="<portlet:namespace />type" value="image"<%= metadataEntry.getDc_type().contains("image") ? " checked" : "" %>> image
					           <input type="checkbox" name="<portlet:namespace />type" value="video"<%= metadataEntry.getDc_type().contains("video") ? " checked" : "" %>> video
					           <input type="checkbox" name="<portlet:namespace />type" value="text"<%= metadataEntry.getDc_type().contains("text") ? " checked" : "" %>> text
					           <input type="checkbox" name="<portlet:namespace />type" value="elo"<%= metadataEntry.getDc_type().contains("elo") ? " checked" : "" %>> elo					                   
					    </td>
                      </tr> 
                </table>