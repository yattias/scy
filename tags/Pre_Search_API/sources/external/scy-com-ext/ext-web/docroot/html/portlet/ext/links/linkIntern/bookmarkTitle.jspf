		<c:if test="<%= sType != null && sType.equals(BookmarksEntry.class.getName()) %>">			
<%		
				List<BookmarksFolder> folderList = (List<BookmarksFolder>)request.getAttribute("folderList");
				BookmarksFolder rootFolder = (BookmarksFolder)request.getAttribute("rootFolder");	
				BookmarksFolder parentFolder = (BookmarksFolder)request.getAttribute("parentFolder");			
				List<BookmarksEntry> bookmarkList = (List<BookmarksEntry>)request.getAttribute("bookmarkList");
%>		
				<c:if test="<%= folderList != null %>">		
					<c:if test="<%= rootFolder.getFolderId() != parentFolder.getFolderId() %>">						
						<portlet:actionURL var="backFolder">
			                    <portlet:param name="struts_action" value="<%= struts_action %>" />
			                    <portlet:param name="entryId" value="<%= String.valueOf(entryId) %>" />
			                  	<portlet:param name="cmd" value="backFolder" />
			                  	<portlet:param name="rootFolderId" value="<%= String.valueOf(rootFolder.getFolderId()) %>" />
			                    <portlet:param name="folderId" value="<%= String.valueOf(parentFolder.getFolderId()) %>" />
			                  	<portlet:param name="sType" value="<%= sType %>" /> 
			                  	<portlet:param name="redirect" value="<%= redirect %>" /> 
			                  	<portlet:param name="className" value="<%= startResourceclassName %>" /> 
						</portlet:actionURL>
						
						<div style="text-align: left; padding:5px;" >		
								<img src="<%= themeDisplay.getURLPortal() %>/html/themes/classic/images/common/folder.png" class="icon"  /> <a href="<%= backFolder %>" <%= onClickHtml %>> ...</a>			
						</div>					
					</c:if>	
<%								
					for (int folderListCount = 0; folderListCount < folderList.size(); folderListCount++) {			
						BookmarksFolder folderEntry = (BookmarksFolder)folderList.get(folderListCount);	
						String folderTitle = folderEntry.getName();		

%>					
					<portlet:actionURL var="documentFolder">
		                    <portlet:param name="struts_action" value="<%= struts_action %>" />
		                    <portlet:param name="entryId" value="<%= String.valueOf(entryId) %>" />
		                  	<portlet:param name="cmd" value="choseFolder" />
		                  	<portlet:param name="rootFolderId" value="<%= String.valueOf(rootFolder.getFolderId()) %>" />
		                    <portlet:param name="folderId" value="<%= String.valueOf(folderEntry.getFolderId()) %>" />
		                  	<portlet:param name="sType" value="<%= sType %>" /> 
		                  	<portlet:param name="redirect" value="<%= redirect %>" /> 
		                  	<portlet:param name="className" value="<%= startResourceclassName %>" /> 
					</portlet:actionURL>
					
					<div style="text-align: left; padding:5px;" >		

							<img src="<%= themeDisplay.getURLPortal() %>/html/themes/classic/images/common/folder.png" class="icon"  /> 
							<a href="<%= documentFolder %>" <%= onClickHtml %>> 								
								 <%= folderTitle %>
							</a>			
					</div>
<%
					}
%>

				</c:if>
				<c:if test="<%= bookmarkList != null %>">			
<%		

					for (int i = 0; i <bookmarkList.size(); i++) {
						BookmarksEntry bookmarkEntry = (BookmarksEntry)bookmarkList.get(i);
					
						String title;
						boolean show = true;
						boolean linkExists= false;
						if(entryId.equals(bookmarkEntry.getEntryId())){
							show= false;
						}
						
					List<LinkEntry> linkExistsList = LinkEntryLocalServiceUtil.getService().getLinkEntry(String.valueOf(entryId), String.valueOf(bookmarkEntry.getEntryId()));
						if (!linkExistsList.isEmpty()) {
									linkExists = true;
						} 	
						
					title = bookmarkEntry.getName();
%>
	
	
					<portlet:actionURL var="bookmarkPreview">
			                    <portlet:param name="struts_action" value="<%= struts_action %>" />
			                    <portlet:param name="entryId" value="<%= String.valueOf(entryId) %>" />
			                  	<portlet:param name="cmd" value="changePreview" />
			                    <portlet:param name="previewEntryId" value="<%= String.valueOf(bookmarkEntry.getEntryId()) %>" />
			                    <portlet:param name="rootFolderId" value="<%= String.valueOf(rootFolder.getFolderId()) %>" />
		                 		<portlet:param name="folderId" value="<%= String.valueOf(parentFolder.getFolderId()) %>" />
			                  	<portlet:param name="sType" value="<%= sType %>" /> 
			                  	<portlet:param name="redirect" value="<%= redirect %>" /> 
			                  	<portlet:param name="className" value="<%= startResourceclassName %>" /> 
					</portlet:actionURL>
	
					<c:if test="<%= show && BookmarksEntryPermission.contains(permissionChecker, bookmarkEntry, ActionKeys.VIEW) %>">
						 <div style="text-align: left; padding:5px;" >					 
								<c:if test="<%= !linkExists %>">
									<a href="<%= bookmarkPreview %>" <%= onClickHtml %>>
										<input type="radio" name="radio" value="<%= bookmarkEntry.getEntryId() %>" <%= checkedResource.equals(String.valueOf(bookmarkEntry.getEntryId())) ? " checked" : "" %> onClick=""> 
										<img src="<%= themeDisplay.getURLPortal() %>/html/themes/classic/images/ratings/star_hover.png" class="icon"  />	
									</a>	
									<a href="<%= bookmarkPreview %>" <%= onClickHtml %>>  <%= title %></a>
								</c:if>							
								<c:if test="<%= linkExists %>">
									<a href="<%= bookmarkPreview %>" <%= onClickHtml %>>
										<input type="radio" name="<%= bookmarkEntry.getEntryId() %>" DISABLED ="true" CHECKED="true" > 
										<img src="<%= themeDisplay.getURLPortal() %>/html/themes/classic/images/ratings/star_hover.png" class="icon"  />		
									</a>	
									<a href="<%= bookmarkPreview %>" <%= onClickHtml %>>  <%= title %></a>
								</c:if>
						</div>
					</c:if>             
					<%
	}
%>
				</c:if>
			</c:if>