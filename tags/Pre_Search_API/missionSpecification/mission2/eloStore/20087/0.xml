<model xmlns="http://www.co-lab.nl/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.co-lab.nl/xml Model.xsd">
  <descriptor>
    <symbol>model</symbol>
  </descriptor>
  <variables>
    <varspec>
      <variable>
        <descriptor>
          <symbol>time</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <timeSpec>
          <unit>?</unit>
          <time start="0.0" stop="10.0" />
          <integration method="euler" step="0.1" />
        </timeSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>mortality_rate</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>0.59</expression>
          <unit>?</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>mortality</symbol>
        </descriptor>
        <type>double</type>
        <color>#fa0000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <auxSpec>
          <expression>mortality_rate*Biomass</expression>
          <unit>?</unit>
          <exprType>0</exprType>
        </auxSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>P</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>3</expression>
          <unit>umol/L</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>N</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>15</expression>
          <unit>umol/L</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>affinity_P</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>0.2</expression>
          <unit>umol/L</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>max_growth_rate</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>1.35</expression>
          <unit>?</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>affinity_N</symbol>
        </descriptor>
        <type>double</type>
        <color>#000000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <constSpec>
          <expression>5.6</expression>
          <unit>umol/L</unit>
        </constSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>growth</symbol>
        </descriptor>
        <type>double</type>
        <color>#ff0000</color>
        <visible>true</visible>
      </variable>
      <specification>
        <auxSpec>
          <expression>Biomass*max_growth_rate*(P/(affinity_P+P))*(N/(affinity_N+N))</expression>
          <unit>?</unit>
          <exprType>0</exprType>
        </auxSpec>
      </specification>
    </varspec>
    <varspec>
      <variable>
        <descriptor>
          <symbol>Biomass</symbol>
        </descriptor>
        <type>double</type>
        <color>#0000ff</color>
        <visible>true</visible>
      </variable>
      <specification>
        <stockSpec>
          <initial>1</initial>
          <unit>10.6 cells/ml</unit>
          <inflow>growth</inflow>
          <outflow>mortality</outflow>
        </stockSpec>
      </specification>
    </varspec>
  </variables>
