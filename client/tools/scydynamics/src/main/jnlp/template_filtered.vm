<jnlp
    spec="$jnlpspec"
    codebase="http://www.scy-lab.eu/scysnippets/scydynamics"
    href="$outputFile">
  <information>
    <title>SCYDynamics modelling tool</title>
    <vendor>University of Twente, Department of Instructional Technology</vendor>
    <homepage href="http://www.scy-lab.eu/scysnippets/scydynamics"/>
    <description>Modelling Tool for System Dynamics models and for qualitative models, usable standalone and within SCYLab</description>
#if($offlineAllowed)
    <offline-allowed/>
#end
  </information>
#if($allPermissions)
  <security>
     <all-permissions/>
  </security>
#end
  <resources>
    <j2se version="$j2seVersion"/>
    <extension name="JavaFX Runtime" href="http://dl.javafx.com/1.1/javafx-rt.jnlp"/>
    <property name="jnlp.packEnabled" value="true"/>
    <property name="jnlp.versionEnabled" value="true"/>
     $dependencies
  </resources>
  <application-desc main-class="$mainClass"/>
</jnlp>

